{
  "name": "BreetApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build-debug": "cd android && ./gradlew clean && cd .. && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleDebug",
    "build-android": "cd android &&  ./gradlew -stop && ./gradlew app:assembleRelease",
    "bundle-android": "cd android &&  ./gradlew -stop && ./gradlew app:bundleRelease",
    "setup": "yarn install && cd ios && pod install --repo-update && cd .. && cd android && ./gradlew clean && cd .. && react-native start --reset-cache",
    "git-setup": "git config --local credential.helper 'cache --timeout=604800'",
    "run-release": "yarn clean-android && react-native run-android --variant=release",
    "clean-android": "cd android && ./gradlew -stop && ./gradlew clean",
    "clean-ios": "cd ios && rm -rf build/generated/ios && rm -rf Pods && rm Podfile.lock",
    "android": "cd android && ./gradlew -stop && cd .. && react-native run-android",
    "ios": "react-native run-ios",
    "connect-android": "adb reverse tcp:8081 tcp:8081",
    "setup-codepush": "yarn global add  appcenter-cli@latest",
    "codepush-android:dev": "appcenter codepush release-react -a The-Tonic-Tech/Cardtonic-Android -d Staging ",
    "codepush-ios:dev": "appcenter codepush release-react -a The-Tonic-Tech/Cardtonic-IOS -d Staging",
    "codepush-android:prod": "appcenter codepush release-react -a The-Tonic-Tech/Cardtonic-Android -d Production",
    "codepush-ios:prod": "appcenter codepush release-react -a The-Tonic-Tech/Cardtonic-IOS -d Production",
    "start": "react-native start --reset-cache",
    "lint": "clear && eslint . --fix && prettier --write '**/*.{js,jsx,ts,tsx}'",
    "test": "jest --watch",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "upgrade-all-dependencies": "yarn yarn-upgrade-all",
    "husky-install": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.8",
    "@react-native-masked-view/masked-view": "^0.3.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/react-native-vector-icons": "^6.4.17",
    "lottie-react-native": "^6.4.0",
    "react": "18.2.0",
    "react-native": "0.73.0",
    "react-native-dotenv": "^3.4.9",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-portalize": "^1.0.7",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-svg": "^13.14.0",
    "react-native-svg-transformer": "^1.1.0",
    "react-native-vector-icons": "^10.0.2",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0"
  },
  "yarn-upgrade-all": {
    "ignore": [
      "react",
      "react-native"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-wildcard": "^7.0.0",
    "babel-plugin-wildcard-import": "^0.2.2",
    "babel-runtime": "^6.26.0",
    "eslint": "^8.19.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
