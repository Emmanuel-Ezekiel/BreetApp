{
  "extends": "@tsconfig/react-native/tsconfig.json",
   "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "*": ["src/*"],
        "tests": ["tests/*"],
        "@components/*": ["src/components/*"],
        "@redux/*": ["src/redux/*"],
        "@assets/*": ["src/assets/*"],
        "@utils/*": ["src/utils/*"],
        "@screens/*": ["src/screens/*"],
        "@services/*": ["src/services/*"],
        "@navigation/*": ["src/navigation/*"],
        "@hooks/*": ["src/hooks/*"],
        "@types/*": ["src/types/*"],
      },
  }
}
